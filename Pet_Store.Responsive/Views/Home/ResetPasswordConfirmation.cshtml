@{
    ViewBag.Title = "Login";
}
<div class="breadcrumb-section breadcrumb-bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 offset-lg-2 text-center">
                <div class="breadcrumb-text">
                    <p>¿Olvidaste tu contraseña?</p>
                </div>
            </div>
        </div>
    </div>
</div>
<br>
<br>

<div class="row justify-content-md-center">
    <div class="login-form text-center pb-80">

        <div class="form-group inputs text-center">
            Se ha cambiado su contraseña con éxito.
        </div>

        <div class="form-group inputs">
            <a id="submit" class="btn btn-outline-primary"
               asp-action="login" asp-controller="home">Volver al inicio de sesión</a>
        </div>
    </div>
</div>



